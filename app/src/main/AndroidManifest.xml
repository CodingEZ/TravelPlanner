<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.baozeng.travelplanner">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".NewPlanActivity"
            android:label="@string/title_activity_new_plan" />
        <activity
            android:name=".ListPlansActivity"
            android:label="@string/title_activity_list_plans" />
        <activity
            android:name=".SaveActivity"
            android:theme="@android:style/Theme.Dialog" />

    </application>

</manifest>